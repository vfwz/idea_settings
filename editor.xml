<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="18" />
    <option name="FONT_FAMILY" value="Consolas" />
    <option name="LINE_SPACING" value="1.1" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern pattern="\bfixme\b.*lzw.*" />
    <pattern pattern="\btodo\b.*lzw.*" />
    <filter name="lzw">
      <pattern index="3" />
      <pattern index="2" />
    </filter>
  </component>
</application>